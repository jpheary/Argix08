<?xml version="1.0" standalone="yes" ?>
<xs:schema id="YardDS" targetNamespace="http://www.tempuri.org/YardDS.xsd" xmlns:mstns="http://www.tempuri.org/YardDS.xsd" xmlns="http://www.tempuri.org/YardDS.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
