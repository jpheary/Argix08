							<xs:element name="Sort" type="xs:string" minOccurs="0" />
							<xs:element name="SortOrder" type="xs:int" minOccurs="0" />
							<xs:element name="GroupBy" type="xs:boolean" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>