<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="BearwareDS" targetNamespace="http://tempuri.org/BearwareDS.xsd" elementFormDefault="qualified"
	attributeFormDefault="qualified" xmlns="http://tempuri.org/BearwareDS.xsd" xmlns:mstns="http://tempuri.org/BearwareDS.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="BearwareDS" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="BwareClientTable">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CLID" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="BwareTripTable">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Number" type="xs:string" minOccurs="0" />
							<xs:element name="CartonCount" type="xs:int" minOccurs="0" />
							<xs:element name="Carrier" type="xs:string" minOccurs="0" />
							<xs:element name="TrailerNumber" type="xs:string" minOccurs="0" />
							<xs:element name="Started" type="xs:dateTime" minOccurs="0" />
							<xs:element name="Stopped" type="xs:dateTime" minOccurs="0" />
							<xs:element name="Exported" type="xs:dateTime" minOccurs="0" />
							<xs:element name="Imported" type="xs:dateTime" minOccurs="0" />
							<xs:element name="Scanned" type="xs:dateTime" minOccurs="0" />
							<xs:element name="OSDSend" type="xs:dateTime" minOccurs="0" />
							<xs:element name="Received" type="xs:dateTime" minOccurs="0" />
							<xs:element name="CartonsExported" type="xs:int" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="BwareStationTripTable">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="StationNumber" type="xs:string" minOccurs="0" />
							<xs:element name="TripNumber" type="xs:string" minOccurs="0" />
							<xs:element name="CartonCount" type="xs:int" minOccurs="0" />
							<xs:element name="Carrier" type="xs:string" minOccurs="0" />
							<xs:element name="TrailerNumber" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="BwareScanTable">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Scan" type="xs:string" minOccurs="0" />
							<xs:element name="TripNumber" type="xs:string" minOccurs="0" />
							<xs:element name="Scanned" type="xs:dateTime" minOccurs="0" />
							<xs:element name="Station" type="xs:string" minOccurs="0" />
							<xs:element name="Cube" type="xs:int" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>