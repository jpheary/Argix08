<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="TripCartons" targetNamespace="http://tempuri.org/TripCartons.xsd" elementFormDefault="qualified"
	attributeFormDefault="qualified" xmlns="http://tempuri.org/TripCartons.xsd" xmlns:mstns="http://tempuri.org/TripCartons.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="TripCartons" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Trip">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="TripNumber" type="xs:string" minOccurs="0" />
							<xs:element name="Carton">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Barcode" type="xs:string" minOccurs="0" />
										<xs:element name="ScanTime" type="xs:dateTime" minOccurs="0" />
										<xs:element name="Cube" type="xs:int" minOccurs="0" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>