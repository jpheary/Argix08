<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="ScannedItemDS"
                  targetNamespace="http://tempuri.org/ScannedItemDS.xsd"
                  elementFormDefault="qualified"
                  attributeFormDefault="qualified"
                  xmlns="http://tempuri.org/ScannedItemDS.xsd"
                  xmlns:mstns="http://tempuri.org/ScannedItemDS.xsd"
                  xmlns:xs="http://www.w3.org/2001/XMLSchema"
                  xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
    <xs:element name="ScannedItemDS" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
        <xs:complexType>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element name="ScannedItemTable" >
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="ItemNumber" type="xs:string" minOccurs="0" />
                            <xs:element name="TerminalID" type="xs:int" minOccurs="0" />
                            <xs:element name="ScannerID" type="xs:string" minOccurs="0" />
                            <xs:element name="FreightID" type="xs:string" minOccurs="0" />
                            <xs:element name="SortDate" type="xs:dateTime" minOccurs="0" />
                            <xs:element name="ScanString" type="xs:string" minOccurs="0" />
                            <xs:element name="UserID" type="xs:string" minOccurs="0" />
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:choice>
        </xs:complexType>
    </xs:element>
</xs:schema>
