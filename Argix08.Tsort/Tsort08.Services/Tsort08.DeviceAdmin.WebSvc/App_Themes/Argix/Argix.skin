<asp:Button Runat="server" Font-Size="1.0em" />
<asp:Calendar runat="server" DayNameFormat="FirstLetter" NextPrevFormat="CustomText" ShowGridLines="True" BorderColor="#E7000D" BorderStyle="Solid" BorderWidth="1px">
    <TodayDayStyle BackColor="#FFE0F3" ForeColor="Black" />
    <DayStyle Font-Size="7pt" ForeColor="Black" />
    <WeekendDayStyle BackColor="#C0C0FF" ForeColor="Black" />
    <NextPrevStyle ForeColor="White" />
    <DayHeaderStyle BackColor="#FFE0F3" Font-Bold="True" ForeColor="Black" />
    <TitleStyle BackColor="#E7000D" Font-Bold="False" Font-Size="1.1em" ForeColor="White" />
</asp:Calendar>
<asp:CheckBox runat="server" Font-Size="1.0em" ForeColor="buttontext" />
