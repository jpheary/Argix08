<asp:CustomValidator Runat="server" Font-Bold="true" ForeColor="#CC3333" BackColor="#ffe0f3" />
<asp:DropDownList Runat="server" Font-Size="1.0em" />
<asp:GridView Runat="server" BorderStyle="Solid" CellPadding="1" CellSpacing="2" BackColor="White" >
    <HeaderStyle BackColor="Control" ForeColor="ControlText" Wrap="True" />
    <PagerStyle BackColor="Control" ForeColor="ControlText" HorizontalAlign="Left" />
</asp:GridView>
