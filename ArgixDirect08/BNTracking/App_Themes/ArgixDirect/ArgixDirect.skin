<asp:HyperLink Runat="server" Font-Names="Verdana" Font-Size="8pt"  ForeColor="#CC3333" />
<asp:LinkButton Runat="server" Font-Names="Verdana" Font-Size="8pt" Font-Bold="true" VerticalAlign="Center"  ForeColor="#CC3333" SkinID="linkBtn" />
